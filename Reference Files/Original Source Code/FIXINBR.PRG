USE PO_HEAD INDEX PO_HEAD

SELECT A
USE INVN_DTL

DO WHILE .NOT. EOF()
   IF I_NBR <> 0
      SKIP
      LOOP
   ENDIF
   IF ACT_CODE <> 'S'
      SKIP
      LOOP
   ENDIF
   
   SELECT B
   USE PO_HEAD INDEX PO_HEAD
   SEEK A->SALESMAN + A->PO_NBR + A->PO_SUFFIX
   
   SELECT E
   USE SUPPLIER INDEX SUP_CODE
   SEEK B->SUP_CODE
   IF .NOT. EOF()
      MFREEZ_LOC = NAME
   ELSE
      MFREEZ_LOC = 'UNKNOWN' + SPACE(25)
   ENDIF
   
   SELECT C
   USE INVEN INDEX INVNLOC
   SEEK MFREEZ_LOC + A->LOT_NBR
